# Quorum RPC URL
QUORUM_RPC=http://127.0.0.1:22000

# Account that sends transactions (unlocked on node) or used for signing
FROM_ADDRESS=0xYourAccountAddressHere
PRIVATE_KEY=0xYourPrivateKeyHere

# Comma-separated Tessera public keys (base64) of recipients for privateFor
PRIVATE_FOR=BULe...base64...,Qz...base64...

# Contract address (for issue script) - saved to deploys/quorum.json after deploy
CONTRACT_ADDRESS=0xDeployedContractAddress

# Credential params for issue script
STUDENT=0xStudentAddress
COURSE=Blockchain 101
GRADE=95

# For grant/revoke
TARGET_ADDRESS=0xAddressToGrantOrRevoke

# For raw tx script
TO_ADDRESS=0xSomeone

# Optional gas
GAS=500000
